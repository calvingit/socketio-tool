{
  "name": "SocketIOClient",
  "version": "1.0.0",
  "description": "SocketIO-Client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish onTagOrDraft"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "zhangwen",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.12"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "socket.io-client": "^2.3.0"
  },
  "build": {
    "appId": "com.electron.socketio",
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage"
      ]
    }
  }
}
